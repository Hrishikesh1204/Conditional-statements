def grades(marks):
  if marks > 90:
    return "A+ grade"
  elif marks > 80:
    return "A grade"
  elif marks > 70:
    return "B grade"
  elif marks > 60:
    return "C grade"
  elif marks > 50:
    return "D grade"
  else:
    return "no grade"
  marks=int(input("enter your marks"))
  print(grades(marks))
